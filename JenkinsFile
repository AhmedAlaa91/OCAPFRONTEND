#!/bin/groovy
pipeline {
    agent any
tools {
    nodejs 'Node'
  }
  stages {
    stage('build') {
      steps {
        
          echo ' Installing NPM'
          bat 'npm install'
        
      }
    }
    stage('Test') {
      steps {
        
          echo ' Runing Test'
          bat 'set CI=true && npm run test'
        
      }
    
    
    }
    
    
    }

  }


